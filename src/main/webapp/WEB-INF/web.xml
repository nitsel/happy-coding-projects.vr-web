<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Book Store</display-name>
  <context-param>
  	  <param-name>Password</param-name>
  	  <param-value>through@pass</param-value>
  </context-param>
  
  <servlet>
      <servlet-name>rating</servlet-name>
      <servlet-class>org.felix.web.servlet.RatingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>rating</servlet-name>
      <url-pattern>/rating.do</url-pattern>
  </servlet-mapping>
  
</web-app>